{
  "extends": 'airbnb',
  "rules": {
    "no-tabs": 0,
    "global-require": 0,
    "no-console": 0,
    "comma-dangle": 0,
    "new-cap": 0,
    "indent": [2, 'tab', { 'SwitchCase': 1 }],
    "no-underscore-dangle": 0,
    "arrow-body-style": [0, 'as-needed'],
    "no-param-reassign": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  }
}
